{
  "name": "storybook-data-json",
  "version": "0.1.0",
  "description": "Present a component's properties in Storybook.",
  "author": "Stefan Natter (http://natterstefan.me)",
  "license": "MIT",
  "main": "index.js",
  "files": ["index.js", "register.js"],
  "scripts": {
    "build": "NODE_ENV=production babel src --ignore */*.test.js,**/*.test.js,*/*.stories.js,**/stories.js -d ./",
