    "lint": "eslint src",
    "watch": "NODE_ENV=production babel -d ./ ./src --watch"
  },
  "dependencies": {
    "@storybook/addons": "^4.1.12",
    "react": "^16.8.2",
    "styled-components": "^4.1.3"
  }
}
