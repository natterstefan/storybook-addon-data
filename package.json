{
  "name": "storybook-addon-data",
  "version": "0.1.0",
  "description": "Present a component's properties in Storybook.",
  "author": "Stefan Natter (http://natterstefan.me)",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": "^10.14",
    "npm": "^6.4",
    "yarn": "^1.9"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --stream --parallel",
    "clean": "lerna clean",
    "lint-js": "eslint ./packages",
    "lint-md": "remark .",
    "postinstall": "yarn bootstrap",
    "prebuild": "copyfiles README.md packages/storybook-addon-data",
    "prepublishOnly": "yarn build && copyfiles README.md packages/storybook-addon-data",
    "pretest": "npm run build",
    "publish": "lerna publish",
    "start": "(cd packages/example-app && yarn start)",
    "start-test-server": "start-server-and-test start http://localhost:9001 test",
    "test": "cypress run",
    "test-cypress": "cypress open",
    "watch": "lerna run watch --stream --parallel"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "yarn lint-js",
      "yarn lint-md",
      "git update-index --again"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natterstefan/storybook-addon-data.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "keywords": [
    "storybook",
    "storybook-addon"
  ],
  "bugs": {
    "url": "https://github.com/natterstefan/storybook-addon-data/issues"
  },
  "homepage": "https://github.com/natterstefan/storybook-addon-data#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.0",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-ns": "^0.4.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.0",
    "lerna": "^3.14.1",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-recommended": "^3.0.2",
    "rimraf": "^2.6.3",
    "start-server-and-test": "^1.9.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  }
}
